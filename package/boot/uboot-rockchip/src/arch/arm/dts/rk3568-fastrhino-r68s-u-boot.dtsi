#include "rk356x-u-boot.dtsi"

&i2c0 {
	bootph-pre-ram;
};

&i2c0_xfer {
	bootph-pre-ram;
};

&pmic_int {
	bootph-pre-ram;
};

&rk809 {
	bootph-pre-ram;

	regulators {
		bootph-pre-ram;
	};
};

&saradc {
	bootph-pre-ram;
};

&sdhci {
	cap-mmc-highspeed;
	mmc-ddr-1_8v;
	mmc-hs200-1_8v;
	mmc-hs400-1_8v;
	mmc-hs400-enhanced-strobe;
	pinctrl-0 = <&emmc_bus8 &emmc_clk &emmc_cmd &emmc_datastrobe>;
};

&usb_host0_xhci {
	dr_mode = "otg";
};

&vcca_1v8 {
	bootph-pre-ram;
};
